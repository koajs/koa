
all: middleware

middleware:
	@./run 1 false $@
	@./run 5 false $@
	@./run 10 false $@
	@./run 15 false $@
	@./run 20 false $@
	@./run 30 false $@
	@./run 50 false $@
	@./run 100 false $@
	@./run 1 true $@
	@./run 5 true $@
	@./run 10 true $@
	@./run 15 true $@
	@./run 20 true $@
	@./run 30 true $@
	@./run 50 true $@
	@./run 100 true $@
	@echo

.PHONY: all middleware
